{
  "name": "@learn-chinese-platform/site",
  "version": "0.0.1-alpha.4",
  "description": "The central access point for all information on the Learn Chinese Platform",
  "author": "Felicitas Pojtinger <felicitas@pojtinger.com>",
  "license": "AGPL-3.0",
  "homepage": "h  ttps://gitlab.com/pojntfx/learn-chinese-platform#README",
  "scripts": {
    "dev": "run-p app.dev--watch",
    "build": "run-p app.prod::build web.prod::build",
    "start": "run-p web.prod::start",
    "install--binary": "sudo install bin/learn-chinese-platform-site /usr/bin",
    "test": "run-p app.prod::test web.prod::test",
    "dev--watch": "run-p dev ",
    "build--watch": "run-p app.prod::build--watch web.prod::build--watch",
    "start--watch": "run-p app.prod::start--watch web.prod::start--watch",
    "test--watch": "run-p app.prod::test--watch web.prod::test--watch",
    "app.prod": "run-p app.prod::build app.prod::start",
    "app.prod::build": "gatsby build",
    "app.prod::start": "gatsby serve",
    "app.prod::test": "echo \"Error: no test specified\" && exit 1",
    "app.prod::build--watch": "nodemon -x 'run-s app.prod::build'",
    "app.prod::start--watch": "run-s app.prod::start",
    "app.prod::test--watch": "run-s app.prod::test",
    "app.dev": "run-p app.dev::start",
    "app.dev::build": "run-s app.prod::build",
    "app.dev::start": "gatsby develop",
    "app.dev::test": "run-s app.prod::test",
    "app.dev--watch": "run-p app.dev::start--watch",
    "app.dev::build--watch": "run-s app.prod::build--watch",
    "app.dev::start--watch": "run-s app.dev::start",
    "app.dev::test--watch": "run-s app.prod::test--watch",
    "web.prod": "run-p web.prod::build web.prod::start",
    "web.prod::build": "mkdir -p public/ && cp src/start.js public/start.js",
    "web.prod::start": "node public/start.js",
    "web.prod::test": "echo \"Error: no test specified\" && exit 1",
    "web.prod--watch": "run-s web.prod::build--watch web.prod::start--watch",
    "web.prod::build--watch": "nodemon -x 'run-s web.prod::build'",
    "web.prod::start--watch": "nodemon -x 'run-s web.prod::start'",
    "web.prod::test--watch": "nodemon -x 'run-s web.prod::test'",
    "web.prod--binary": "run-s web.prod::build--binary web.prod::start--binary",
    "web.prod::build--binary": "mkdir -p bin/ && pkg package.json -t node10-linux-x64 -o bin/learn-chinese-platform-site",
    "web.prod::start--binary": "chmod +x bin/learn-chinese-platform-site && bin/learn-chinese-platform-site",
    "web.prod::build--watch--binary": "nodemon -x 'run-s web.prod::build--binary'",
    "web.prod::start--watch--binary": "nodemon -x 'run-s web.prod::start--binary'",
    "web.dev": "run-p web.dev::build web.dev::start",
    "web.dev::build": "run-s web.prod::build",
    "web.dev::start": "node src/start.js",
    "web.dev::test": "run-s web.prod::test",
    "web.dev--watch": "run-p web.dev::build--watch web.dev::start--watch",
    "web.dev::build--watch": "run-s web.prod::build--watch",
    "web.dev::start--watch": "nodemon -x 'run-s web.dev::start'",
    "web.dev::test--watch": "run-s web.prod::test--watch"
  },
  "dependencies": {
    "@libresat/frontend-components": "^0.0.1-75",
    "@mdx-js/mdx": "^0.15.0",
    "@mdx-js/tag": "^0.15.0",
    "copy-webpack-plugin": "^4.6.0",
    "core-js": "^2.5.7",
    "emotion": "^9.2.12",
    "emotion-server": "^9.2.12",
    "express": "^4.16.4",
    "gatsby": "^2.0.62",
    "gatsby-link": "^2.0.7",
    "gatsby-mdx": "0.2.0",
    "gatsby-plugin-emotion": "^2.0.7",
    "gatsby-plugin-manifest": "^2.0.11",
    "gatsby-plugin-offline": "^2.0.18",
    "gatsby-plugin-react-helmet": "^3.0.4",
    "gatsby-source-filesystem": "^2.0.10",
    "gatsby-transformer-yaml": "^2.1.6",
    "react": "^16.6.3",
    "react-dom": "^16.6.3",
    "react-emotion": "^9.2.12",
    "react-helmet": "^5.2.0",
    "react-masonry-component": "^6.2.1",
    "reactjs-coverflow": "^2.1.3",
    "semantic-ui-react": "^0.83.0"
  },
  "devDependencies": {
    "nodemon": "^1.18.7",
    "npm-run-all": "^4.1.5",
    "pkg": "^4.3.5"
  },
  "main": "public/start.js",
  "source": "src/start.js",
  "bin": "public/start.js",
  "pkg": {
    "assets": [
      "public/**/*"
    ],
    "scripts": "public/start.js"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@gitlab.com/pojntfx/learn-chinese-platform.git"
  },
  "bugs": {
    "url": "https://gitlab.com/pojntfx/learn-chinese-platform/issues"
  },
  "keywords": [
    "chinese"
  ],
  "publishConfig": {
    "access": "public"
  }
}
