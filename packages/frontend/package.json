{
  "name": "@learn-chinese-platform/frontend",
  "version": "0.0.1-alpha.12",
  "description": "Frontend for @learn-chinese-platform, a modern and libre way to learn Chinese.",
  "author": "Felicitas Pojtinger <felicitas@pojtinger.com>",
  "license": "AGPL-3.0",
  "homepage": "https://gitlab.com/pojntfx/learn-chinese-platform#README",
  "scripts": {
    "dev": "run-p app.dev--watch",
    "build": "run-p app.prod::build web.prod::build",
    "start": "run-p web.prod::start",
    "install--binary": "sudo install bin/learn-chinese-platform-frontend /usr/bin",
    "test": "run-p app.prod::test web.prod::test",
    "deploy": "exoframe",
    "dev--watch": "run-p dev",
    "build--watch": "run-p app.prod::build--watch web.prod::build--watch",
    "start--watch": "run-p app.prod::start--watch web.prod::start--watch",
    "test--watch": "run-p app.prod::test--watch web.prod::test--watch",
    "app.prod": "run-s app.prod::build app.prod::start",
    "app.prod::build": "parcel build src/index.html",
    "app.prod::start": "parcel src/index.html",
    "app.prod::test": "echo \"Error: no test specified\" && exit 1",
    "app.prod::build--watch": "parcel watch src/index.html",
    "app.prod::start--watch": "run-s app.prod::start",
    "app.prod::test--watch": "run-s app.prod::test",
    "app.dev": "run-s app.dev:build app.dev::start",
    "app.dev::build": "run-s app.prod::build",
    "app.dev::start": "run-s app.prod::start",
    "app.dev::test": "run-s app.prod::test",
    "app.dev--watch": "run-p app.dev:build--watch app.dev::start--watch",
    "app.dev::build--watch": "run-s app.prod::build--watch",
    "app.dev::start--watch": "run-s app.prod::start--watch",
    "app.dev::test--watch": "run-s app.prod::test--watch",
    "web.prod": "run-s web.prod::build web.prod::start",
    "web.prod::build": "tsc --outDir dist/ src/start.ts",
    "web.prod::start": "node dist/start.js",
    "web.prod::test": "echo \"Error: no test specified\" && exit 1",
    "web.prod--watch": "run-s web.prod::build--watch web.prod::start--watch",
    "web.prod::build--watch": "tsc -w --outDir dist/ src/start.ts",
    "web.prod::start--watch": "nodemon -x 'run-s web.prod::start'",
    "web.prod::test--watch": "nodemon -x 'run-s web.prod::test'",
    "web.prod--binary": "run-s web.prod::build--binary web.prod::start--binary",
    "web.prod::build--binary": "mkdir -p bin/ && pkg package.json -t node10-linux-x64 -o bin/learn-chinese-platform-frontend",
    "web.prod::start--binary": "chmod +x bin/learn-chinese-platform-frontend && bin/learn-chinese-platform-frontend",
    "web.prod::build--watch--binary": "nodemon -x 'run-s web.prod::build--binary'",
    "web.prod::start--watch--binary": "nodemon -x 'run-s web.prod::start--binary'",
    "web.dev": "run-s web.dev::build web.dev::start",
    "web.dev::build": "run-s web.prod::build",
    "web.dev::start": "ts-node -P tsconfig.json src/start.ts",
    "web.dev::test": "run-s web.prod::test",
    "web.dev--watch": "run-p web.dev::build--watch web.dev::start--watch",
    "web.dev::build--watch": "run-s web.prod::build--watch",
    "web.dev::start--watch": "nodemon -x 'run-s web.dev::start'",
    "web.dev::test--watch": "run-s web.prod::test--watch",
    "prepare": "run-p build"
  },
  "dependencies": {
    "@libresat/frontend-components": "^0.0.1-75",
    "@mdx-js/tag": "^0.15.0",
    "express": "^4.16.4",
    "react": "^16.5.2",
    "react-copy-to-clipboard": "^5.0.1",
    "react-dom": "^16.5.2",
    "react-router-dom": "^4.3.1",
    "semantic-ui-react": "^0.83.0",
    "styled-components": "^4.0.2"
  },
  "devDependencies": {
    "@mdx-js/parcel-plugin-mdx": "^0.15.6",
    "@types/express": "^4.16.0",
    "@types/react": "^16.4.18",
    "@types/react-copy-to-clipboard": "^4.2.6",
    "@types/react-dom": "^16.0.9",
    "@types/react-router-dom": "^4.3.1",
    "@types/styled-components": "^4.1.2",
    "babel-core": "^6.26.3",
    "nodemon": "^1.18.4",
    "npm-run-all": "^4.1.3",
    "parcel-bundler": "^1.10.3",
    "pkg": "^4.3.4",
    "ts-node": "^7.0.1",
    "typescript": "^3.2.2"
  },
  "main": "src/index.tsx",
  "source": "src/index.tsx",
  "bin": "dist/start.js",
  "pkg": {
    "assets": [
      "dist/**/*"
    ],
    "scripts": "dist/start.js"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@gitlab.com/pojntfx/learn-chinese-platform.git"
  },
  "bugs": {
    "url": "https://gitlab.com/pojntfx/learn-chinese-platform/issues"
  },
  "keywords": [
    "chinese"
  ],
  "publishConfig": {
    "access": "public"
  }
}
